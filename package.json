{
  "name": "ikon",
  "version": "1.0.0",
  "description": "A tool for building icon  spritesheets",
  "author": {
    "name": "Oliver Turner",
    "email": "oliver.turner@codedsignal.co.uk",
    "url": "https://www.oliverturner.cloud"
  },
  "repository": {
    "url": "git@github.com:oliverturner/ikon.git"
  },
  "homepage": "https://ikon.oliverturner.cloud",
